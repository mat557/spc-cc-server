{
    "version": 2,
    "builds": [
        { "src": "package.json", "use": "@vercel/static-build", "development": true },
        { "src": "package.json", "use": "@vercel/static-build", "development": false, "outputDirectory": "public" }
      ],
    "routes": [
       { 
        "src": "/(.*)", 
        "dest": "server.js"
        }
    ]
 }